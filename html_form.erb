<html>
<head>
  <meta charset='utf-8'>
  <title>Attendees</title>
</head>
<body>
  <table style="width:100%" border="1" >
    <tr bgcolor="#FF0000">
      <th>Last Name</th>
      <th>First Name</th>
      <th>Email</th>
      <th>Zipcode</th>
      <th>City</th>
      <th>State</th>
      <th>Address</th>
      <th>Phone</th>
     <!-- <th>district</th> -->
    </tr>

  <% @csv_prepped.each do |row| %>
    <tr>
      <td><%= "#{row[0]}" %></td>
      <td><%= "#{row[1]}" %></td>
      <td><%= "#{row[2]}" %></td>
      <td><%= "#{row[3]}" %></td>
      <td><%= "#{row[4]}" %></td>
      <td><%= "#{row[5]}" %></td>
      <td><%= "#{row[6]}" %></td>
      <td><%= "#{row[7]}" %></td>
    </tr>
    <% end %>
  </table>

</body>
</html>
